<Window x:Class="PhishTest.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PhishTest"
        mc:Ignorable="d"
        Title="Phishing Security Test" Height="222.413" Width="478.9">
	<Grid>
		<Label x:Name="label" Content="Oops! You downloaded a file from a phishing link" HorizontalAlignment="Center" Margin="10,10,10,0" VerticalAlignment="Top" Width="443" FontWeight="Bold" Background="#FFFF9191"/>
		<Label x:Name="label1" Content="Don't worry. This is a phishing security test by the Information Security Team.&#xD;&#xA;Remember:&#xD;&#xA;* Check the sender email.&#xD;&#xA;* Hover over the any link to verify it is safe.&#xD;&#xA;* Do not enter your credentials unless you triple check the website URL.&#xD;&#xA;* Do not download program and run them without approved Software Request." HorizontalAlignment="Left" Margin="10,41,0,0" VerticalAlignment="Top" Width="443" Height="118" Background="#FFFFECBA"/>
		<Label x:Name="footer" Content="(c) COMPANYNAME 2019" HorizontalAlignment="Center" Margin="10,159,10,0" VerticalAlignment="Top" Height="25" Width="443" FontSize="10" Background="{x:Null}"/>

	</Grid>
</Window>
